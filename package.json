{
  "name": "ts-lib-template",
  "version": "1.0.2",
  "description": "ts-lib-template desc",
  "types": "./dist/types/main.d.ts",
  "main": "./dist/cli.js",
  "browser": "./dist/main.browser.js",
  "module": "./dist/main.es.js",
  "exports": {
    ".": {
      "import": "./dist/main.es.js",
      "require": "./dist/cli.js"
    }
  },
  "bin": "./dist/cli.js",
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "start": "esbuild src/main.ts --outfile=dist/main.dev.js --platform=browser --global-name=logger_ts  --bundle --sourcemap=external --serve=2021 --servedir=dist",
    "prestart": "npm run esbuild-main:dev && shx cp -rf public/* dist/ && node node/open.js",
    "esbuild-main:dev": "esbuild src/main.ts --platform=browser --global-name=logger_ts --bundle  --sourcemap=external --outfile=dist/main.dev.js",
    "test": "jest",
    "lint": "eslint --max-warnings 0 . --ext .ts",
    "build": "tsc -p tsconfig.json",
    "clean": "shx rm -rf dist build",
    "ts-node": "ts-node",
    "build-all": "npm run clean && npm run build && npm run esbuild-node && npm run esbuild-browser && npm run esbuild-esm",
    "esbuild-browser": "esbuild src/browser.ts --platform=browser --global-name=logger_ts --bundle --minify --outfile=dist/main.browser.js",
    "esbuild-browser:dev": "esbuild src/browser.ts --platform=browser --global-name=logger_ts --bundle  --sourcemap=external --outfile=dist/main.browser.js",
    "esbuild-node": "esbuild src/cli.ts --bundle --platform=node --minify --outfile=dist/cli.js",
    "esbuild-node:dev": "esbuild src/cli.ts --bundle --platform=node --sourcemap=external --outfile=dist/cli.js",
    "esbuild-esm": "esbuild src/es.ts --platform=neutral --bundle --minify --outfile=dist/main.es.js",
    "esbuild-esm:dev": "esbuild src/es.ts --platform=neutral --bundle --sourcemap=external --outfile=dist/main.es.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shirtiny/ts-lib-template"
  },
  "keywords": [
    "consloe",
    "log",
    "ts",
    "logger",
    "TypeScript",
    "shirtiny"
  ],
  "author": {
    "name": "shirtiny",
    "email": "shirtiny@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shirtiny/ts-lib-template/issues"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "homepage": "https://github.com/Shirtiny/ts-lib-template#readme",
  "devDependencies": {
    "@cowcoders/eslint-config": "^1.2.9",
    "@types/jest": "26.0.23",
    "@types/node": "15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "esbuild": "0.12.5",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "27.0.3",
    "open": "^8.2.0",
    "prettier": "^2.3.0",
    "shx": "^0.3.3",
    "ts-jest": "27.0.1",
    "ts-node": "10.0.0",
    "typescript": "^4.3.3"
  }
}
